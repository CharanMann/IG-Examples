{
  "baseURI": "https://openam-inspirus-dev.id.forgerock.io",
  "capture": "all",
  "condition": "${matches(request.uri.path,'^/am')}",
  "handler": {
    "type": "Chain",
    "config": {
      "filters": [
        {
          "name": "hostHeader",
          "type": "HeaderFilter",
          "config": {
            "messageType": "REQUEST",
            "remove": [ "host" ],
            "add": {
              "host": [
                "openam-inspirus-dev.id.forgerock.io"
              ]
            }
          }
        }
      ],
      "handler": "FRClientHandler"
    }
  }
}
